#ssh
alias ussh='printf "Unsafe SSH - Do not trust any messages about the key being saved. It wont be :)\\n"; ssh -o UserKnownHostsFile=/dev/null -o CheckHostIP=no -o StrictHostKeyChecking=no'
alias ssh.x='ssh -c arcfour,blowfish-cbc -XC'
alias ssh.list='cat ~/.ssh/config | grep -i "host "|cut -d" " -f2'
